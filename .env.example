# Development environment variables
# Copy this file to .env.local and modify as needed

# Application settings
VITE_APP_TITLE=ASUSWRT-Merlin NginxUI
VITE_APP_VERSION=1.0.0

# API settings
VITE_API_BASE_URL=/start_apply.htm
VITE_API_TIMEOUT=30000

# Development settings
VITE_DEV_MODE=true
VITE_DEBUG=false

# Build settings
VITE_BUILD_TARGET=es2015
VITE_BUILD_MINIFY=true

# Router settings
VITE_ROUTER_BASE=/
VITE_ROUTER_MODE=hash

# Logging
VITE_LOG_LEVEL=info