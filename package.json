{
  "name": "asuswrt-merlin-nginxui",
  "version": "1.0.0",
  "description": "A lightweight and efficient Web UI for managing Nginx service on ASUSWRT-Merlin routers",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:prod": "NODE_ENV=production vue-tsc && vite build --mode production",
    "build:package": "./build.tar.sh",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:style": "stylelint \"src/**/*.{css,scss,vue}\" --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "type-check": "vue-tsc --noEmit",
    "clean": "rm -rf dist build asuswrt-merlin-nginxui.tar.gz",
    "analyze": "vite build --mode analyze"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "pinia": "^2.1.0",
    "vue": "^3.4.0",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.12.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/vue": "^8.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.5.0",
    "@vue/vue3-jest": "^29.2.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.69.0",
    "stylelint": "^16.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-order": "^6.0.0",
    "stylelint-scss": "^6.0.0",
    "terser": "^5.43.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.0",
    "vite": "^5.0.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vue-tsc": "^1.8.0"
  },
  "keywords": [
    "asuswrt-merlin",
    "nginx",
    "router",
    "web-ui",
    "vue",
    "typescript"
  ],
  "author": "NasPilot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NasPilot/asuswrt-merlin-nginxui.git"
  },
  "bugs": {
    "url": "https://github.com/NasPilot/asuswrt-merlin-nginxui/issues"
  },
  "homepage": "https://github.com/NasPilot/asuswrt-merlin-nginxui#readme"
}
